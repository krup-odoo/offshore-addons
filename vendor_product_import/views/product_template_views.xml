<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_view_form" model="ir.ui.view">
        <field name="name">product.template.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="sale_product_configurator.product_template_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook/page[@name='invoicing']" position="after">
                <page string="Import History">
                    <field name="vendor_product_ids">
                        <tree editable="bottom">
                            <field name="import_reference"/>
                            <field name="date_of_import"/>
                            <field name="filename"/>
                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
